{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://gabl.ink/cms/dictionaries/schemas/disclaimer.json",
  "title": "Disclaimers",
  "description": "Disclaimers",
  "type": "object",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "patternProperties": {
        "^[a-z]{2,3}(-([A-Z]{2}|[0-9]{3}))?$": {
          "type": "string",
          "pattern": "^.*$"
        }
      }
    }
  }
}
